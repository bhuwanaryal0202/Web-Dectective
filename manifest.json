{
    "manifest_version": 3,
    "name": "Website dectective",
    "version": "1.0",
    "description": "A Chrome extension to pick website details",
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
 